# Ângulo.
import math
an = float(input('Digite um ângulo'))
seno = math.sin(math.radians(an))
print('O ângulo de {} ten o SENO de {:.2f}'.format(an, seno))
cosseno = math.cos(math.radians(an))
print('O ângulo de {} ten o COSSENO de {:.2f}'.format(an, cosseno))
tangente = math.tan(math.radians(an))
print('O ângulo de {} ten o TANGENTE de {:.2f}'.format(an, tangente))
